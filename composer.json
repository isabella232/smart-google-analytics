{
    "name": "zealousweb/smart-google-analytics",
    "description": "Smart Google Analytics",
    "type": "craft-plugin",
    "version": "2.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft4",
        "craft-plugin",
        "Smart Google Analytics"
    ],
    "support": {
        "docs": "https://github.com/zealousweb/smart-google-analytics/blob/master/README.md",
        "issues": "https://github.com/zealousweb/smart-google-analytics/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "zealousweb",
            "homepage": "https://www.zealousweb.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "craftcms/cms": "^4.0.0",
        "google/apiclient": "^v2.6.0",
        "google/analytics-data": "^0.8.6",
        "google/apiclient-services":"^0.262.0",
        "google/auth":"^1.21.1",
        "google/analytics-admin":"^0.6.4",
        "craftcms/html-field": "^2.0.0"
    },
    "require-dev": {
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "zealousweb\\smartgoogleanalytics\\": "src/"
        }
    },
    "extra": {
        "name": "Smart Google Analytics",
        "handle": "smart-google-analytics",
        "developer": "zealousWeb",
        "developerUrl": "https://www.zealousweb.com",
        "documentationUrl": "https://github.com/zealousweb/smart-google-analytics/blob/master/README.md",
        "changelogUrl": "https://github.com/zealousweb/smart-google-analytics/blob/master/CHANGELOG.md",
        "class": "zealousweb\\smartgoogleanalytics\\SmartGoogleAnalytics"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
